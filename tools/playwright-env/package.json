{
  "name": "@j5/playwright-env",
  "private": true,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@dopplerhq/node-sdk": "1.3.0",
    "@t3-oss/env-core": "0.13.10",
    "simple-git": "3.30.0",
    "tslib": "2.8.1",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.14.10"
  },
  "peerDependencies": {
    "@nx/devkit": "21.4.1"
  },
  "executors": "./executors.json",
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "tools/playwright-env/dist",
          "main": "tools/playwright-env/src/index.ts",
          "tsConfig": "tools/playwright-env/tsconfig.lib.json",
          "rootDir": "tools",
          "generatePackageJson": false,
          "assets": [
            {
              "input": "./tools/playwright-env/src",
              "glob": "**/!(*.ts)",
              "output": "."
            },
            {
              "input": "./tools/playwright-env/src",
              "glob": "**/*.d.ts",
              "output": "."
            }
          ]
        }
      }
    }
  }
}
