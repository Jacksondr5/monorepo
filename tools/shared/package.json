{
  "name": "@j5/shared-tools",
  "private": true,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@dopplerhq/node-sdk": "1.3.0",
    "@nx/devkit": "22.3.3",
    "simple-git": "3.30.0",
    "tslib": "2.8.1"
  },
  "executors": "./executors.json",
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "tools/shared/dist",
          "main": "tools/shared/src/index.ts",
          "tsConfig": "tools/shared/tsconfig.lib.json",
          "rootDir": "tools/shared/src",
          "generatePackageJson": false,
          "assets": [
            {
              "input": "./tools/shared/src",
              "glob": "**/!(*.ts)",
              "output": "."
            },
            {
              "input": "./tools/shared/src",
              "glob": "**/*.d.ts",
              "output": "."
            }
          ]
        }
      }
    }
  }
}
