{
  "mcpServers": {
    "nx-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "nx-mcp@latest"
      ]
    },
    "convex": {
      "command": "pnpm dlx",
      "args": [
        "-y",
        "convex@latest",
        "mcp",
        "start"
      ]
    },
    "browsermcp": {
      "command": "pnpm dlx",
      "args": [
        "@browsermcp/mcp@latest"
      ]
    }
  }
}